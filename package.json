{
  "name": "592-pinedale",
  "version": "1.0.0",
  "description": "An in-house menu app",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "seed": "node scripts/seedDB.js",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "if-env NODE_ENV=production && npm run build:prod || npm run build:dev",
    "build:prod": "cd client && npm run build:prod",
    "build:dev": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n8blake/592-pinedale.git"
  },
  "keywords": [
    "menu",
    "service",
    "drink",
    "drinks",
    "food",
    "beverage",
    "alcohol",
    "wine",
    "list"
  ],
  "author": "n8blake",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/n8blake/592-pinedale/issues"
  },
  "homepage": "https://github.com/n8blake/592-pinedale#readme",
  "devDependencies": {
    "concurrently": "^7.1.0",
    "express-http-proxy": "^1.6.3",
    "nodemon": "^2.0.16"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^13.3.4",
    "@angular/cli": "^13.3.4",
    "@angular/compiler-cli": "^13.3.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "if-env": "^1.0.4",
    "mongoose": "^6.3.2",
    "mongoose-extend-schema": "^1.0.0",
    "path": "^0.12.7",
    "typescript": "~4.5.2"
  }
}
